print @l int @r(4 + 5);
print @l int @r(4 - 5); /* -1 */
print @l int @r(4 * 5);
print @l int @r(4 / 2);
print @l bool @r(4 == 5); /* false */
print @l bool @r(4 == 4); /* true */
print @l bool @r(4 != 5); /* true */
print @l bool @r(5 != 5); /* false */
print @l bool @r(4 < 5);  /* true */
print @l bool @r(4 < 3);  /* false */
print @l bool @r(4 <= 5); /* true */
print @l bool @r(4 <= 4); /* true */
print @l bool @r(4 >= 5); /* false */
print @l bool @r(4 > 5);  /* false */
print @l bool @r(5 >= 5); /* true */