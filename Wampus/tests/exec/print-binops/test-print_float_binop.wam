print @l float @r(4.0 + 5.0);
print @l float @r(4.0 - 5.0); /* -1.0 */
print @l float @r(4.0 * 5.0);
print @l float @r(4.0 / 2.0);
print @l bool @r(4.0 == 5.0); /* false */
print @l bool @r(4.0 == 4.0); /* true */
print @l bool @r(4.0 != 5.0); /* true */
print @l bool @r(5.0 != 5.0); /* false */
print @l bool @r(4.0 < 5.0);  /* true */
print @l bool @r(4.0 < 3.0);  /* false */
print @l bool @r(4.0 <= 5.0); /* true */
print @l bool @r(4.0 <= 4.0); /* true */
print @l bool @r(4.0 >= 5.0); /* false */
print @l bool @r(4.0 > 5.0);  /* false */
print @l bool @r(5.0 >= 5.0); /* true */