template @l T, hello @r
T test(T x, hello y) {
    T w = x;
    hello z = y;
    return w;
}

printi(test @l int, int @r (5, 4));
printi(test @l int, int @r (4, 5));
printi(test @l int, int @r (3, 6));
printi(test @l int, int @r (2, 7));
printi(test @l int, bool @r (5, true));
printi(test @l int, char @r (3, 'a'));
printi(test @l int, float @r (2, 7.91));
printi(test @l int, string @r (8, "not"));

printb(test @l bool, int @r (true, 4));
printb(test @l bool, bool @r (true, false));
printb(test @l bool, char @r (true, 'a'));
printb(test @l bool, float @r (false, 1.23));
printb(test @l bool, string @r (false, "false"));

printc(test @l char, int @r ('a', 4));
printc(test @l char, bool @r ('w', true));
printc(test @l char, char @r ('a', 'b'));
printc(test @l char, float @r ('q', 3.14));
printc(test @l char, string @r ('b', "bee"));

printf(test @l float, int @r (1.2, 4));
printf(test @l float, bool @r (6.78, true));
printf(test @l float, char @r (123.456, 'y'));
printf(test @l float, float @r (1.0, 1.0));
printf(test @l float, string @r (08234.87, "maybe yes"));

printfs(test @l string, int @r ("hello", 2));
printfs(test @l string, bool @r ("hello", false));
printfs(test @l string, char @r ("world", '!'));
printfs(test @l string, float @r ("alive", 456.73));
printfs(test @l string, string @r ("work", "please"));
